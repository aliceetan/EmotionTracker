<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hu.ait.android.emotiontracker">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".FeelActivity"
            android:label="@string/title_activity_how_do_you_feel"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ActionActivity"
            android:label="@string/title_activity_action"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".data.Emotion" />
        <activity android:name=".actions.HappyActions"></activity>
    </application>

</manifest>